{
  "profiles": [
    {
      "name": "access-to-finland",
      "services": [
        "authentication-gw",
        "users-api",
        "testbed-api",
        "external-service-demo",
        "access-to-finland-demo-front",
        "status-info-api",
        "status-admin",
        "codesets",
        "tmt-productizer",
        "JobsInFinland.Api.Productizer"
      ]
    },
    {
      "name": "access-finland",
      "services": ["authentication-gw", "testbed-api", "access-finland", "codesets", "prh-mock", "users-api"]
    },
    {
      "name": "virtual-finland-demo",
      "services": ["authentication-gw", "testbed-api", "virtual-finland-demo", "codesets", "prh-mock", "users-api"]
    },
    {
      "name": "external-service-demo",
      "services": ["authentication-gw", "users-api", "testbed-api", "external-service-demo", "status-info-api", "status-admin", "codesets"]
    },
    {
      "name": "status-admin",
      "services": ["authentication-gw", "status-info-api", "status-admin"]
    }
  ],
  "dockerComposeOverrides": {
    "alwaysRebuild": ["users-api", "prh-mock", "tmt-productizer", "JobsInFinland.Api.Productizer"],
    "environment": [
      {
        "key": "USERAPI_DOCKERFILE",
        "value": "Dockerfile.arm64",
        "conditions": [
          {
            "key": "service",
            "value": "users-api"
          },
          {
            "key": "arch",
            "value": "aarch64"
          }
        ]
      }
    ]
  },
  "vfd-ssh-git": "git@github.com:Virtual-Finland-Development"
}
